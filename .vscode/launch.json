{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "NestJS Debug",
      "runtimeExecutable": "${workspaceFolder}/.yarn/releases/yarn-4.6.0.cjs",
      "runtimeArgs": ["start:debug"],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "NODE_ENV": "development",
        "ENV_FROM": "file",
      },
    }
  ]
} 